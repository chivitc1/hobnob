{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Backend",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/backend",
      "runtimeExecutable": "${workspaceFolder}/backend/node_modules/.bin/babel-node",
      "runtimeArgs": [
        "--presets",
        "@babel/env"
      ],
      "program": "${file}",
      "protocol": "inspector",
      "env": {
        "BABEL_ENV": "debug",
        "SERVER_PORT": "8080",
        "SERVER_PROTOCOL": "http",
        "SERVER_HOSTNAME": "localhost",
        "MONGODB_HOST": "localhost",
        "MONGODB_PORT": "27017",
        "DBNAME": "hobnob",
        "MONGODB_USER": "user1",
        "MONGODB_PASSWORD": "password123"
      }
    }
  ],
  "compounds": []
}