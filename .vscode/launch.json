{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Backend",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/backend",
      "runtimeExecutable": "${workspaceFolder}/backend/node_modules/.bin/babel-node",
      "runtimeArgs": [
        "--presets",
        "@babel/env"
      ],
      "program": "${workspaceFolder}/backend/src/index.js",
      "protocol": "inspector",
      "env": {
        "BABEL_ENV": "debug"
      }
    }
  ],
  "compounds": []
}