{
  "name": "backend",
  "version": "1.0.0",
  "description": "Learn react - backend",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && babel src -d dist --copy-files",
    "serve": "yarn run build && dotenv -e envs/run.env node dist/index.js",
    "start": "dotenv -e envs/.env babel-node src/index.js",
    "fix": "eslint src --fix",
    "lint": "eslint src",
    "start:prod": "scripts/run-backend.sh",
    "start:dev": "nodemon -w src --exec yarn start",
    "test:e2e": "scripts/test-e2e.sh",
    "test:unit": "mocha 'src/**/*.test.js' --require @babel/register",
    "test": "yarn run test:unit && yarn run test:e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chivitc1/hobnob.git"
  },
  "keywords": [
    "react",
    "express",
    "hobnob"
  ],
  "author": "chivitc1",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chivitc1/hobnob/issues"
  },
  "homepage": "https://github.com/chivitc1/hobnob#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "cucumber": "^5.0.3",
    "dotenv-cli": "^1.4.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "mocha": "^5.2.0",
    "ndb": "^1.0.42",
    "nodemon": "^1.18.9",
    "rimraf": "^2.6.2",
    "sinon": "^7.2.2",
    "superagent": "^4.0.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "mongoose": "^5.4.0"
  }
}
