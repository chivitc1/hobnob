{
  "version": "0.2.0",
  "configurations": [    
    {
      "name": "Babel Node",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      "runtimeArgs": [
        "--presets",
        "@babel/env"
      ],
      "program": "${file}",
      "protocol": "inspector",
      "env": {
        "BABEL_ENV": "debug"
      }
    }
  ],
  "compounds": []
}